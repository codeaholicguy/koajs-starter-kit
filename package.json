{
  "name": "koajs-starter-kit",
  "version": "1.0.0",
  "description": "Simple starter kit for using KoaJS to build a web application server written in ES7.",
  "main": "index.js",
  "repository": "https://github.com/codeaholicguy/koajs-starter-kit.git",
  "author": "Hoang Nhat Nguyen <hoangnn93@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node",
